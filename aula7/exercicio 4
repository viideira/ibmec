def converter_temperatura():
    celsius = float(input("Digite a temperatura em graus Celsius: "))
    fahrenheit = (celsius * 9/5) + 32
    print(f"A temperatura em Fahrenheit Ã©: {fahrenheit:.2f}")

# Exemplo de uso
converter_temperatura()

